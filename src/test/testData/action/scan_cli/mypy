#!/bin/bash
if [ "$1" = "-V" ]; then
  echo "mypy 1.11.2 (compiled: yes)"
else
  my_dir=$(dirname "$(realpath -s "$0")")
  for last; do :; done
  expected_args="--show-column-numbers --show-absolute-path --output json --ignore-paths /src/excluded_dir $last"
  if [ "$*" != "$expected_args" ]; then
    >&2 echo "expected: $expected_args"
    >&2 echo "received: $*"
    exit 2 # because 1 needs to be treated as being fine
  fi
  cat "$my_dir/manualScan.json"
fi